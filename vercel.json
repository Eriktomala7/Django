{
  "version": 2,
  "builds": [
    {
      "src": "DjangoProjectFinal/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "python": {
          "runtime": "python3.12"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/aplicaciones/pagina_web/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/DjangoProjectFinal/wsgi.py"
    }
  ],
  "buildCommand": "rm -rf node_modules && bash vercel_build.sh",
  "installCommand": "pip install --no-cache-dir -r requirements.txt",
  "regions": ["iad1"],
  "public": true,
  "env": {
    "PYTHONUNBUFFERED": "1",
    "PYTHONPATH": "/var/task/DjangoProjectFinal:/var/task/aplicaciones"
  }
}
