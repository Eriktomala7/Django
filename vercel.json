{
  "version": 2,
  "builds": [
    {
      "src": "DjangoProjectFinal/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "python": {
          "runtime": "python3.10"
        }
      }
    },
    {
      "src": "aplicaciones/pagina_web/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/aplicaciones/pagina_web/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/DjangoProjectFinal/wsgi.py"
    }
  ],
  "buildCommand": "bash vercel_build.sh",
  "installCommand": "pip install -r requirements.txt",
  "outputDirectory": "staticfiles"
}
