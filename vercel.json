{
  "version": 2,
  "builds": [
    {
      "src": "vercel.json",
      "use": "@vercel/python"
    },
    {
      "src": "aplicaciones/pagina_web/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/aplicaciones/pagina_web/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
